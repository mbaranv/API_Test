pipeline {
    agent any
    tools {
            maven 'maven-3.8.3'

    }
    stages {
        stage('build') {
            steps {
                echo 'Compiling the java source code'
                bat 'mvn compile'

            }
        }
        stage('run') {
            steps {
                echo 'Running the compiled java code.'
                bat 'mvn test'

            }
        }
           stage('report') {
                    steps {

                                    // Test aşaması
                                    // Test çerçevesinden gelen sonuçları toplamak için report-aggregate eklentisini kullanabilirsiniz
                                    reportAggregate(

                                    )
                                }

                }
    }
}