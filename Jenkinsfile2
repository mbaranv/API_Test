pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Compiling the java source code'
                sh 'javac API_Test/src/test/java/allRequest/UserService.java'
            }
        }
        stage('run') {
            steps {
                echo 'Running the compiled java code.'
                sh 'java API_Test/src/test/java/allRequest/UserService.java'
            }
        }
    }
}