pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Compiling the java source code'
                bat 'javac Users/Baran/IdeaProjects/API_Test/src/test/java/allRequest/UserService.java'
            }
        }
        stage('run') {
            steps {
                echo 'Running the compiled java code.'
                bat 'java Users/Baran/IdeaProjects/API_Test/src/test/java/allRequest/UserService'
            }
        }
    }
}