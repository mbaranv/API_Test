pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Compiling the java source code'
                sh 'mvn install clean test'
                bat 'javac javac src\\test\\java\\allRequest\\UserService.java'
            }
        }
        stage('run') {
            steps {
                echo 'Running the compiled java code.'
                bat 'java javac src\\test\\java\\allRequest\\UserService'
            }
        }
    }
}