pipeline {
    agent any
    tools {
            maven 'Maven'
    }
    stages {
        stage('build') {
            steps {
                echo 'Compiling the java source code'
                bat 'javac src\\test\\java\\allRequest\\UserService.java'
            }
        }
        stage('run') {
            steps {
                echo 'Running the compiled java code.'
                bat 'java src\\test\\java\\allRequest\\UserService'
            }
        }
    }
}