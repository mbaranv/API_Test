pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Compiling the java source code'
                sh 'javac home/ec2-user/API_Test/src/test/java/allRequest/Hello.java'
            }
        }
        stage('run') {
            steps {
                echo 'Running the compiled java code.'
                sh 'java home/ec2-user/API_Test/src/test/java/allRequest/Hello'
            }
        }
    }
}