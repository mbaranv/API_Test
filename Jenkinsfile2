pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Compiling the java source code'
                sh 'javac src/test/java/allRequest/UserService.java'
            }
        }
        stage('run') {
            steps {
                echo 'Running the compiled java code.'
                sh 'java -cp src/test/java/allRequest/UserService'
            }
        }
    }
}