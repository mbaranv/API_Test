pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Compiling the java source code'
                bat 'javac -cp C:/ProgramData/Jenkins/.jenkins/workspace/APITest02/src/test/java/allRequest/UserService.java'
            }
        }
        stage('run') {
            steps {
                echo 'Running the compiled java code.'
                bat 'java -cp C:/ProgramData/Jenkins/.jenkins/workspace/APITest02/src/test/java/allRequest/UserService.java'
            }
        }
    }
}